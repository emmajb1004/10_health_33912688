<!doctype html>
<html>
  <head>
    <title>Register</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-sRIl4kxILFvY47J16cr9ZwB07vP4J8+LH7qKQnuqkuIAvNWLzeN8tE5YBujZqJLB" crossorigin="anonymous">
  </head>
  <body>
    <div class="container">
      <h1 class="mb-4">Register</h1>
      <%# Form for registration %>
      <form method="POST" action="/users/registered">
          <p>First name: <input id="firstName" type="text" name="firstName" /></p>
          <p>Last name: <input id="lastName" type="text" name="lastName" /></p>
          <p>Email: <input id="email" type="email" name="email" /></p>
          <p>Username: <input id="username" type="text" name="username" /></p>
          <p>Password: <input id="password" type="text" name="password" /></p>
          <input type="submit" value="Register" />
        </form>     
      </div>
  </body>
</html>